num1 = int(input())
num2 = int(input())
num3 = int(input())

if num1 < num2 < num3:
  print(str(num2))

elif num3 < num2 < num1:
  print(str(num2))

elif num2 < num1 < num3:
  print(str(num1))

elif num3 < num1 < num2:
  print(str(num1))

elif num2 < num3 < num1:
  print(str(num1))

elif num1 < num3 < num2:
  print(str(num1))
